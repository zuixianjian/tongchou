<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.exrick.xboot.your.mapper.CheckCodeMapper">

    <select id="getTodayCode" resultType="cn.exrick.xboot.your.entity.CheckCode">
        select * from b_check_code where del_flag=0
        and to_days(generate_date) = to_days(now());
    </select>
</mapper>